<div class="container">
  <h1>Here are your lists:</h1>
  <ul>
    <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %>, <%= link_to "Delete", list_path(list), data: { turbo_method: :delete, turbo_confirm: "Are you sure you want to delete this bookmark?"}%></li>
    <% end %>
  </ul>
</div>

<%= link_to "Create a new list", new_list_path %>
